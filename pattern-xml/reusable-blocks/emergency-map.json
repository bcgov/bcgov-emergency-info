  "content": "<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"},\"margin\":{\"top\":\"var:preset|spacing|30\",\"bottom\":\"var:preset|spacing|30\"}}},\"backgroundColor\":\"white\",\"layout\":{\"type\":\"constrained\"}} -->\n<div id=\"Emergency-Map\" class=\"wp-block-group has-white-background-color has-background\" style=\"margin-top:var(--wp--preset--spacing--30);margin-bottom:var(--wp--preset--spacing--30);padding-top:0;padding-right:0;padding-bottom:0;padding-left:0\"><!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"var:preset|spacing|20\",\"right\":\"var:preset|spacing|20\",\"bottom\":\"var:preset|spacing|20\",\"left\":\"var:preset|spacing|20\"},\"margin\":{\"top\":\"0\",\"bottom\":\"0\"}}},\"className\":\"alignwide\",\"layout\":{\"type\":\"default\"}} -->\n<div class=\"wp-block-group alignwide\" style=\"margin-top:0;margin-bottom:0;padding-top:var(--wp--preset--spacing--20);padding-right:var(--wp--preset--spacing--20);padding-bottom:var(--wp--preset--spacing--20);padding-left:var(--wp--preset--spacing--20)\"><!-- wp:areoi/row {\"block_id\":\"b095be9f-2a28-4f03-8117-5a114ba3027f\",\"row_cols_xs\":\"row-cols-1\",\"row_cols_md\":\"row-cols-md-2\"} -->\n<!-- wp:areoi/column {\"block_id\":\"13558b72-5366-4cc3-aa61-c20316cc344e\",\"background_color\":{\"rgb\":{\"r\":0,\"g\":0,\"b\":0,\"a\":1}},\"background_overlay\":{\"rgb\":{\"r\":0,\"g\":0,\"b\":0,\"a\":1}},\"order_xs\":\"order-2\",\"order_sm\":\"order-sm-2\",\"order_md\":\"order-md-0\",\"className\":\"my-1\"} -->\n<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"}}},\"className\":\"h-100\",\"layout\":{\"type\":\"constrained\"}} -->\n<div class=\"wp-block-group h-100\" style=\"padding-top:0;padding-right:0;padding-bottom:0;padding-left:0\"><!-- wp:cover {\"url\":\"https://test.vanity.blog.gov.bc.ca/app/uploads/sites/1187/2023/09/x-block.png\",\"id\":45857,\"dimRatio\":0,\"focalPoint\":{\"x\":0,\"y\":0},\"className\":\"h-100 d-flex align-items-start emergency-map\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"var:preset|spacing|30\",\"right\":\"var:preset|spacing|30\",\"bottom\":\"var:preset|spacing|30\",\"left\":\"var:preset|spacing|30\"}}}} -->\n<div class=\"wp-block-cover h-100 d-flex align-items-start emergency-map\" style=\"padding-top:var(--wp--preset--spacing--30);padding-right:var(--wp--preset--spacing--30);padding-bottom:var(--wp--preset--spacing--30);padding-left:var(--wp--preset--spacing--30)\"><span aria-hidden=\"true\" class=\"wp-block-cover__background has-background-dim-0 has-background-dim\"></span><img class=\"wp-block-cover__image-background wp-image-45857\" alt=\"\" src=\"https://test.vanity.blog.gov.bc.ca/app/uploads/sites/1187/2023/09/x-block.png\" style=\"object-position:0% 0%\" data-object-fit=\"cover\" data-object-position=\"0% 0%\"/><div class=\"wp-block-cover__inner-container\"><!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"},\"margin\":{\"bottom\":\"var:preset|spacing|20\"}}},\"layout\":{\"type\":\"constrained\",\"contentSize\":\"400px\",\"justifyContent\":\"right\"}} -->\n<div class=\"wp-block-group\" style=\"margin-bottom:var(--wp--preset--spacing--20);padding-top:0;padding-right:0;padding-bottom:0;padding-left:0\"><!-- wp:group {\"backgroundColor\":\"white\",\"layout\":{\"type\":\"default\"}} -->\n<div class=\"wp-block-group has-white-background-color has-background\"><!-- wp:heading {\"style\":{\"spacing\":{\"padding\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"}}},\"textColor\":\"text-color\"} -->\n<h2 class=\"wp-block-heading has-text-color-color has-text-color\" style=\"padding-top:0;padding-right:0;padding-bottom:0;padding-left:0\">Get Updates on X (Twitter)</h2>\n<!-- /wp:heading -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"margin\":{\"bottom\":\"var:preset|spacing|20\"}}},\"layout\":{\"type\":\"flex\",\"flexWrap\":\"nowrap\"}} -->\n<div class=\"wp-block-group\" style=\"margin-bottom:var(--wp--preset--spacing--20)\"><!-- wp:areoi/icon {\"block_id\":\"4bc5af99-90a0-4d44-bca4-e940f72de678\",\"style\":\"text-dark\",\"icon\":\"bi-link\"} -->\n<div class=\"wp-block-areoi-icon areoi-icon\"><i class=\"text-dark bi-link \" style=\"font-size:24px\"></i></div>\n<!-- /wp:areoi/icon -->\n\n<!-- wp:paragraph {\"style\":{\"spacing\":{\"margin\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"}}},\"textColor\":\"text-color\",\"className\":\"text-break\"} -->\n<p class=\"text-break has-text-color-color has-text-color\" style=\"margin-top:0;margin-right:0;margin-bottom:0;margin-left:0\">Follow @EmergencyInfoBC for updates.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:group -->\n\n<!-- wp:areoi/button {\"block_id\":\"008d20b5-cf0b-4c94-b163-67b91fb5102a\",\"style\":\"btn-warning\",\"url\":\"https://twitter.com/EmergencyInfoBC\",\"text\":\"\\u003cstrong\\u003eOpen X (Twitter)\\u003c/strong\\u003e\",\"include_icon\":true,\"icon\":\"bi-arrow-up-right\",\"className\":\"text-primary\"} /--></div>\n<!-- /wp:group --></div>\n<!-- /wp:group --></div></div>\n<!-- /wp:cover --></div>\n<!-- /wp:group -->\n<!-- /wp:areoi/column -->\n\n<!-- wp:areoi/column {\"block_id\":\"0d0fb620-afdb-4176-82dd-11a334bc9e05\",\"background_color\":{\"rgb\":{\"r\":0,\"g\":0,\"b\":0,\"a\":1}},\"background_overlay\":{\"rgb\":{\"r\":0,\"g\":0,\"b\":0,\"a\":1}},\"order_xs\":\"order-0\",\"order_sm\":\"order-sm-0\",\"order_md\":\"order-md-2\",\"className\":\"my-1\"} -->\n<!-- wp:group {\"style\":{\"border\":{\"radius\":\"10px\"},\"spacing\":{\"padding\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"}}},\"backgroundColor\":\"primary-brand\",\"className\":\"h-100\",\"layout\":{\"type\":\"constrained\"}} -->\n<div class=\"wp-block-group h-100 has-primary-brand-background-color has-background\" style=\"border-radius:10px;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0\"><!-- wp:cover {\"url\":\"https://test.vanity.blog.gov.bc.ca/app/uploads/sites/1187/2023/04/image-6-1024x1019-1.png\",\"id\":1552,\"dimRatio\":0,\"isDark\":false,\"className\":\"h-100 d-flex align-items-start emergency-map\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"var:preset|spacing|30\",\"right\":\"var:preset|spacing|30\",\"bottom\":\"var:preset|spacing|30\",\"left\":\"var:preset|spacing|30\"}}}} -->\n<div class=\"wp-block-cover is-light h-100 d-flex align-items-start emergency-map\" style=\"padding-top:var(--wp--preset--spacing--30);padding-right:var(--wp--preset--spacing--30);padding-bottom:var(--wp--preset--spacing--30);padding-left:var(--wp--preset--spacing--30)\"><span aria-hidden=\"true\" class=\"wp-block-cover__background has-background-dim-0 has-background-dim\"></span><img class=\"wp-block-cover__image-background wp-image-1552\" alt=\"\" src=\"https://test.vanity.blog.gov.bc.ca/app/uploads/sites/1187/2023/04/image-6-1024x1019-1.png\" data-object-fit=\"cover\"/><div class=\"wp-block-cover__inner-container\"><!-- wp:group {\"layout\":{\"type\":\"constrained\",\"contentSize\":\"400px\",\"justifyContent\":\"right\"}} -->\n<div class=\"wp-block-group\"><!-- wp:group {\"style\":{\"spacing\":{\"margin\":{\"bottom\":\"var:preset|spacing|20\"}}},\"backgroundColor\":\"white\",\"layout\":{\"type\":\"default\"}} -->\n<div class=\"wp-block-group has-white-background-color has-background\" style=\"margin-bottom:var(--wp--preset--spacing--20)\"><!-- wp:heading {\"style\":{\"spacing\":{\"padding\":{\"top\":\"0\",\"right\":\"0\",\"bottom\":\"0\",\"left\":\"0\"}}}} -->\n<h2 class=\"wp-block-heading\" style=\"padding-top:0;padding-right:0;padding-bottom:0;padding-left:0\">Find Emergencies Near You</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>Learn about public safety conditions near you. Emergency data may not always be available in real-time. Use only for reference.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:areoi/button {\"block_id\":\"008d20b5-cf0b-4c94-b163-67b91fb5102a\",\"style\":\"btn-warning\",\"url\":\"https://governmentofbc.maps.arcgis.com/apps/webappviewer/index.html?id=950b4eec577a4dc5b298a61adab41c06\",\"text\":\"\\u003cstrong\\u003eOpen Emergency Map\\u003c/strong\\u003e\",\"include_icon\":true,\"icon\":\"bi-arrow-up-right\",\"className\":\"text-primary\"} /--></div>\n<!-- /wp:group --></div>\n<!-- /wp:group --></div></div>\n<!-- /wp:cover --></div>\n<!-- /wp:group -->\n<!-- /wp:areoi/column -->\n<!-- /wp:areoi/row --></div>\n<!-- /wp:group --></div>\n<!-- /wp:group -->"
