{
	"name": "bcgov-emergency-info",
	"version": "1.0.0",
	"author": "Bcgov Wordpress <govwordpress@gov.bc.ca>",
	"repository": {
		"type": "git",
		"url": "https://bitbucket.org:bc-gov/bcgov-emergency-info.git"
	},
	"bugs": {
		"url": "https://bitbucket.org:bc-gov/bcgov-emergency-info/jira"
	},
	"licenses": "MIT",
	"scripts": {
		"build": "wp-scripts build --output-path=dist",
		"format": "wp-scripts format src",
		"lint:css": "wp-scripts lint-style 'src/**/*.scss'",
		"fix:css": "wp-scripts lint-style 'src/**/*.scss' --fix",
		"lint:js": "wp-scripts lint-js src/scripts",
		"fix:js": "wp-scripts lint-js src/scripts --fix",
		"lint:md": "wp-scripts lint-md-docs",
		"fix:md": "wp-scripts lint-md-docs --fix",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start --output-path=dist",
		"test:e2e": "wp-scripts test-e2e",
		"test": "wp-scripts test-unit-js"
	},
	"engines": {
		"npm": ">=10.0.0 <11.0.0",
		"node": ">=20.0.0 <22.0.0"
	},
	"devDependencies": {
		"@bcgov/wordpress-eslint": "git+https://github.com/bcgov/wordpress-eslint.git#semver:1.x",
		"@bcgov/wordpress-stylelintrc": "git+https://github.com/bcgov/wordpress-stylelintrc.git#semver:1.x",
		"@wordpress/scripts": "^28.6.0"
	},
	"dependencies": {
		"bootstrap": "5.0.2"
	},
	"browser": {
		"[module-name]": false
	}
}
