{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "title": "Event Meta Block",
  "name": "emergency-info/event-meta",
  "category": "emergency-info",
  "icon": "embed-photo",
  "textdomain": "emergency-info",
  "usesContext": [
    "postId",
    "postType"
  ],
  "attributes": {
    "hazard_types": {
      "type": "object",
      "default": {}
    },
    "detailed": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}